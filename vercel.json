{
  "version": 2,
  "installCommand": "npm install --ignore-scripts",
  "builds": [
    {
      "src": "api/[...path].ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/[...path]"
    },
    {
      "src": "/api",
      "dest": "/api/[...path]"
    }
  ]
}
